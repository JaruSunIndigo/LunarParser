<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.4</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Sergio Flores</Authors>
    <Company>Lunar Labs</Company>
    <Product>Lunar Parser</Product>
    <Description>Read data from strings in XML/JSON formats into a tree structure. 
Create data nodes manually and serialize it back to XML/JSON.</Description>
    <PackageProjectUrl>https://github.com/relfos/lunar_parser</PackageProjectUrl>
    <RepositoryUrl>https://github.com/relfos/lunar_parser</RepositoryUrl>
    <Version>1.0.4</Version>
    <AssemblyVersion>1.0.4.0</AssemblyVersion>
    <PackageReleaseNotes>Rewrite of XML parser, faster and with support for comments</PackageReleaseNotes>
    <FileVersion>1.0.4.0</FileVersion>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>TRACE;DEBUG;NETSTANDARD1_4;DATETIME_AS_TIMESTAMPS</DefineConstants>
  </PropertyGroup>

</Project>
